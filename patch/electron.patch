+    this.onClicked = {
+      addListener(cb) {
+        self.onClickedEvent.add(cb)
+      },
+      removeListener(cb){
+        self.onClickedEvent.delete(cb)
+      },
+      hasListener(cb){
+        return self.onClickedEvent.has(cb)
+      },
+      hasListeners(){
+        return !!self.onClickedEvent.length
+      }
